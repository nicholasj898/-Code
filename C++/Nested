#include <iostream>
#include <string>

using namespace std;

int main()
{
    int i = 0;
    while (i <= 45) {              
        cout << i << " " << endl;  
        i+=5;                      
    }                              
    while (i <= 100) {
        cout << i << " " << endl;
        i+=1;
    }
}